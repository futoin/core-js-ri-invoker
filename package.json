{
  "name": "futoin-invoker",
  "version": "0.8.2",
  "description": "FutoIn Invoker - Reference Implementation. Transparently and efficiently invoke remote or local service methods with strict API definition for Node and Browser",
  "main": "lib/invoker.js",
  "browser": "dist/invoker-amd.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint *.js lib --reporter=./node_modules/jshint-full-path/index.js",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec test/spectooltest.js test/unittest.js",
    "posttest": "./node_modules/.bin/istanbul check-coverage && ./node_modules/.bin/istanbul report html || true"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/futoin/core-js-ri-invoker"
  },
  "keywords": [
    "futoin",
    "async",
    "rpc",
    "orb",
    "communication",
    "distributed",
    "browser",
    "amd"
  ],
  "author": {
    "name": "Andrey Galkin",
    "email": "andrey@futoin.eu"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/futoin/core-js-ri-invoker/issues",
    "email": "support@futoin.eu"
  },
  "dependencies": {
    "agentkeepalive": "^1.2.0",
    "detect-node": "^2.0.3",
    "event-emitter": "^0.3.3",
    "faye-websocket": "^0.9.3",
    "futoin-asyncsteps": "^1.5.6",
    "keep-alive-agent": "*",
    "lodash": "^4.13.1",
    "request": "^2.53.0",
    "performance-now": "*"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt": "~0.4.5",
    "grunt-contrib-connect": "~0.9.0",
    "grunt-contrib-uglify": "~0.8.0",
    "grunt-external-daemon": "^1.1.0",
    "grunt-jsdoc-to-markdown": "^1.2.1",
    "grunt-mocha-phantomjs": "^3.0.0 ",
    "grunt-npm-helper": "~0.0.2",
    "grunt-npm2bower-sync": "^0.8.1",
    "grunt-pure-cjs": "~1.4.0",
    "grunt-release": "~0.11.0",
    "grunt-text-replace": "*",
    "istanbul": "^0.3.6",
    "jscs": "^1.11.3",
    "jsdoc": "^3.2.2",
    "jsdoc-to-markdown": "^1.3.6",
    "jshint": "^2.6.3",
    "jshint-full-path": "*",
    "mocha": "^2.5.3"
  }
}
