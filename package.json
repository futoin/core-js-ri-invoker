{
  "name": "futoin-invoker",
  "version": "0.5.6",
  "description": "FutoIn Invoker - Reference Implementation. Transparently and efficiently invoke remote or local service methods with strict API definition for Node and Browser",
  "main": "lib/invoker.js",
  "browser": "dist/invoker-amd.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint lib --reporter=./node_modules/jshint-full-path/index.js",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec test/spectooltest.js test/unittest.js",
    "posttest": "./node_modules/.bin/istanbul check-coverage && ./node_modules/.bin/istanbul report html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/futoin/core-js-ri-invoker"
  },
  "keywords": [
    "futoin",
    "async",
    "rpc",
    "orb",
    "communication",
    "distributed",
    "browser",
    "amd"
  ],
  "author": {
    "name": "Andrey Galkin",
    "email": "andrey@futoin.eu"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/futoin/core-js-ri-invoker/issues",
    "email": "support@futoin.eu"
  },
  "dependencies": {
    "agentkeepalive": "^1.2.0",
    "detect-node": "^2.0.3",
    "event-emitter": "^0.3.1",
    "faye-websocket": "^0.9.2",
    "futoin-asyncsteps": "^1.5.1",
    "keep-alive-agent": "*",
    "lodash": "*",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "chai": "*",
    "grunt": "*",
    "grunt-contrib-connect": "*",
    "grunt-contrib-uglify": "*",
    "grunt-external-daemon": "^1.1.0",
    "grunt-mocha-phantomjs": "*",
    "grunt-npm-helper": "*",
    "grunt-npm2bower-sync": "0.4.0",
    "grunt-pure-cjs": "*",
    "grunt-release": "*",
    "grunt-text-replace": "*",
    "grunt-jsdoc-to-markdown": "*",
    "istanbul": "*",
    "jscs": "*",
    "jsdoc": "~3.2",
    "jsdoc-to-markdown": "^0.5.9",
    "jshint": "*",
    "jshint-full-path": "*",
    "mocha": "*",
    "mocha-phantomjs": "*",
    "performance-now": "*"
  }
}
