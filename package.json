{
  "name": "futoin-invoker",
  "version": "0.1.0",
  "description": "FutoIn Invoker - Reference Implementation",
  "main": "lib/invoker.js",
  "browser": "dist/invoker-amd.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint lib --reporter=./node_modules/jshint-full-path/index.js",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec test/unittest.js",
    "posttest": "./node_modules/.bin/istanbul check-coverage && ./node_modules/.bin/istanbul report html",
    "doc": "./node_modules/.bin/jsdoc2md -t misc/README.hbs lib/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/futoin/core-js-ri-invoker"
  },
  "keywords": [
    "futoin",
    "async",
    "rpc",
    "communication",
    "distributed",
    "browser",
    "amd"
  ],
  "author": {
    "name": "Andrey Galkin",
    "email": "andrey@futoin.eu"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/futoin/core-js-ri-invoker/issues",
    "email": "support@futoin.eu"
  },
  "dependencies": {
    "agentkeepalive": "^1.2.0",
    "detect-node": "^2.0.3",
    "futoin-asyncsteps": "^1.3.1",
    "keep-alive-agent": "0.0.1",
    "lodash": "^2.4.1",
    "ws": "^0.4.32"
  },
  "devDependencies": {
    "chai": "*",
    "grunt": "*",
    "grunt-contrib-connect": "*",
    "grunt-contrib-uglify": "*",
    "grunt-mocha-phantomjs": "^*",
    "grunt-npm-helper": "0.0.2",
    "grunt-npm2bower-sync": "^0.4.0",
    "grunt-pure-cjs": "^1.4.0",
    "grunt-release": "^0.9.0",
    "istanbul": "*",
    "jscs": "*",
    "jsdoc": "~3.2",
    "jsdoc-to-markdown": "^0.5.9",
    "jshint": "*",
    "jshint-full-path": "*",
    "mocha": "*",
    "mocha-phantomjs": "*",
    "performance-now": "*"
  }
}
